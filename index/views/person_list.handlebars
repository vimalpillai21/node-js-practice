<html>
<head>
    <title>
        Person List
    </title>
</head>
<body>
<div class="container">
    <div class="row">
    <table class="table table-responsive">
        <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Nationality</th>
        </tr>
        </thead>
        <tbody>
        {{#each person}}
            <tr>
                <td>{{name}}</td>
                <td>{{age}}</td>
                <td>{{nationality}}</td>
            </tr>

        {{/each}}

        </tbody>
    </table>
    </div>
</div>
</body>
</html>
